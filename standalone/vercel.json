{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "dashboard/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "dist/index.js"
    },
    {
      "src": "/dashboard",
      "dest": "dashboard/index.html"
    },
    {
      "src": "/",
      "dest": "dashboard/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "$1"
    }
  ]
}

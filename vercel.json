{
  "version": 2,
  "buildCommand": "bash build.sh",
  "outputDirectory": "public",
  "functions": {
    "api/index.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    { "src": "/api/networks", "methods": ["GET"], "dest": "/api/index.js" },
    { "src": "/api/subgraphs/(.*)", "methods": ["GET"], "dest": "/api/index.js" },
    { "src": "/api/match", "methods": ["GET", "POST", "OPTIONS"], "dest": "/api/index.js" },
    { "src": "/chat", "dest": "/dashboard/chat.html" },
    { "src": "/dashboard/chat", "dest": "/dashboard/chat.html" },
    { "src": "/", "dest": "/dashboard/index.html" }
  ]
}
